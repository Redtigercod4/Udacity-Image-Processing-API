{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "env": {
    "node": true
  },
  "rules": {
    "@typescript-eslint/no-var-requires": 0
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"]
}
